{
    "manifest_version": 3,
    "name": "Price Addition Extension",
    "version": "1.0",
    "description": "Extension to add price to specific blocks on a webpage",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  