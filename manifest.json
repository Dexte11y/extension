{
    "manifest_version": 3,
    "name": "Best price",
    "version": "1.0",
    "description": "The extension finds the best prices for goods on the website https://optvideo.com/",
    "permissions": [],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://optvideo.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  
